<?xml version="1.0" encoding="utf-8" ?>
<configuration>
   <startup>
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
   </startup>

   <system.diagnostics>
      <trace autoflush="true">
         <listeners>
            <add name="MyNLogTraceListener" type="NLog.NLogTraceListener, NLog" />
            <remove name="Default" />
         </listeners>
      </trace>
   </system.diagnostics>

</configuration>